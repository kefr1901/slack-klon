<!DOCTYPE html>
<html>

<head>
  <title> </title>

  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <!-- Head navigation -->


  <div class="sidenav">

    <img id="side_logo" src="images/talck_logo.png">
    <h2 id="logo_text">SNACK</h2>
    <br>
    <button id="add_room_btn">Lägg till rum!</button>
    <a href="#">#Channel</a>
    <a href="#">#Skitsnack</a>
    <hr>
    <br>
    <!-- Side navigation-->

    <ul>
      <% for(let i=0; i < data.length; i++){ %>
        <li> <a href="#"><%= data[i].username %></li></a>
     <% } %>
    </ul>

  </div>

  <!-- Main -->
  <div class="main">
    <div id="chatbox">
      <label id="username_chat">Username är inloggad</label>
      <div id="msg-cont">

      </div>
    </div>

  </div>

  <form method="POST" id="snd-cont" name="" action="/chat">

    <input id="msg-inp" type="text" name="msg-inp" placeholder="Skriv ditt meddelande.."></input><button id="snd-btn"
      type="submit">Skicka</button>

  </form>

  <script defer src="http://localhost:3000/socket.io/socket.io.js"></script>
  <script defer src="/script.js"></script>


</body>

</html>